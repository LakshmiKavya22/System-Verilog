
class vlsi;
  int a;
  int b;
endclass
module tb;
  vlsi v1,v2;
  initial begin
    v1=new();
    v2=new();
    v2.a=10;
    v1=new v2;
    $display("V1=%0d",v1.a);
    $display("V1=%0p",v1);
    v2.a=20;
    $display("V1=%0p",v1);
  end
endmodule
//In shallow coping separate memory locations are allocated for destionation and source handle.
