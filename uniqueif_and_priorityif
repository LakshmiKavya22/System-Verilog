module tb;
  initial begin
    int a=50;
    int b=100;
    unique if(a==b)
      $display("a is Equal to b");
    else if(a<b)
      $display("a is less than b");
    else if(a>100)
      $display("a is greater than 100");
    else
      $display("Exit condition");
  end
endmodule

OUTPUT: a less than b

